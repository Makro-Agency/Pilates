      "type": "tonic-bookings",
      "settings": {
        "sub_heading": "EVENT SPACE RENTAL",
        "heading": "UNIQUE EVENT & PARTY VENUE IN THE HEART OF TORONTO",
        "description": "<p>Host your next team meeting, cocktail, holiday, or office party at Tonic House - our multi-purpose, fully-licensed, boutique event space and community hub amidst a downtown view.</p>",
        "btn_label": "VIEW EVENTS PACKAGE",
        "btn_link": "https://bit.ly/sweatandtonic-event-package",
        "desktop_slider": true,
        "mobile_slider": false
      }
