        "faq_fDa8A4": {
          "type": "faq",
          "settings": {
            "question": "PREMIUM FULL-SERVICE AMENITIES",
            "ans": "<p>Heated flooring, changing rooms, showers, towel service, grip socks for purchase, and full-service machine cleaning between every class. Come as you are, and we'll do the rest</p>"
          }
        },
        "faq_GJQnjH": {
