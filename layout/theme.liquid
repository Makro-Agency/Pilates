  </head>
  {% assign has_page_font = false %}
  {% if template.name == 'page' %}
    {% assign has_page_font = true %}
  {% endif %}
  <body class="gradient pp--{{ has_page_font }} {% if settings.animations_hover_elements != 'none' %} animate--hover-{{ settings.animations_hover_elements }}{% endif %} template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }} template--{{ template.suffix }}">
    <!--  -->
    {% comment %} <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KBWHHMM"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) --> {% endcomment %}
    
        
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ 'accessibility.skip_to_text' | t }}
    </a>

    {%- if settings.cart_type == 'drawer' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}

    {% sections 'header-group' %}
    {%- liquid            
      assign has_drakness = false
      assign bright_temp = 'index,pre-sale-landing,faq,terms-conditions' | split:','
      for bt in bright_temp
        if bt == "page"
          if template.suffix == blank
            assign has_drakness = true
            break
          endif
        else
          if template contains bt
            assign has_drakness = true
            break
          endif
        endif
      endfor
    -%}
    {% if has_drakness %}
      {% style %}
        :root, .color-background-1 {
          --color-background: 0, 0, 0;
          --gradient-background: #000;
          --color-foreground: 255, 255, 255;          
          --color-shadow: 255, 255, 255;
          --color-button: 255, 255, 255;
          --color-button-text: 0, 0, 0;
          --color-secondary-button: 0, 0, 0;
          --color-secondary-button-text: 199, 157, 91;
          --color-link: 199, 157, 91;
          --color-badge-foreground: 255, 255, 255;
          --color-badge-background: 0, 0, 0;
          --color-badge-border: 255, 255, 255;
          --payment-terms-background-color: rgb(255 255 255);
        }
        body .policies-section .rte {
          color: rgb(var(--color-foreground));
        }
      {% endstyle %}
    {% endif %}

    <main id="MainContent" class="content-for-layout focus-none dark--{{ has_drakness }}" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    <div class="drawer-classes-btns sticky-classes-btns large-up-hide medium-hide">
      <a href="https://shop.sweatandtonic.com/pages/reserve" class="button btn-small-width left-btn">book classes</a>
      <a href="https://shop.sweatandtonic.com/pages/pricing" class="button btn-small-width right-btn">buy classes</a>
    </div>

    {% sections 'footer-group' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>

    {%- render 'global-js-var' -%}

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    
    <script src="{{ 'custom.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'custom-ipstudio.js' | asset_url }}" defer="defer"></script>

    
    <!-- Check User is logged in -->
    <script>
      document.addEventListener("DOMContentLoaded", function(event){
        myAccount();
      });
      
      function myAccount(){
        var userLoggedDetail = localStorage.getItem("mt.https://sweatandtonic.marianatek.com");        
        var hasLoaggedIn = false;        
        if( userLoggedDetail != null ){
          userLoggedDetail = JSON.parse(userLoggedDetail);          
          if( typeof userLoggedDetail.userId != 'undefined' && userLoggedDetail.userId != null ){
            hasLoaggedIn = true;
            if( document.querySelector('.header__icon--account .header__icon_text') ){
              document.querySelector('.header__icon--account .header__icon_text').innerText = '{{ 'customer.account_fallback' | t }}';
            }
          }
        }else{
          var _locationSearch = window.location.search;
          if( _locationSearch ){
            if( _locationSearch.match('account') != null ){
              hasLoaggedIn = true;
              if( document.querySelector('.header__icon--account .header__icon_text') ){
                document.querySelector('.header__icon--account .header__icon_text').innerText = '{{ 'customer.account_fallback' | t }}';
              }            
            }
          }
        };
        if( window.location.pathname.match(/account/ig) != null && !hasLoaggedIn ){
          window.myAccountCount = typeof window.myAccountCount == 'undefined' ? 0 : window.myAccountCount;
          if(window.myAccountCount < 6){
            window.myAccountCount = window.myAccountCount + 1;
            setTimeout(function(){ myAccount(); }, 1500);
          }
        }
      };
    </script>
    <!-- Check User is logged in -->
    
  </body>
</html>